  "scripts": {
    "build": "pnpm install -r && pnpm run -r build",
    "build:api": "cd packages/react-google-maps-api && pnpm install && pnpm run build && cd ../..",
    "build:docs": "cd packages/react-google-maps-api && pnpm install && pnpm run docs:build && cd ../..",
    "build:clusterer": "cd packages/react-google-maps-api-marker-clusterer && pnpm install && pnpm run build && cd ../..",
    "build:infobox": "cd packages/react-google-maps-api-infobox && pnpm install && pnpm run build && cd ../..",
    "build:gatsby": "cd packages/react-google-maps-api-gatsby-example && pnpm install && pnpm run build && cd ../..",
    "prebuild:docs": "pnpm run build:clusterer && pnpm run build:infobox",
    "prebuild:gatsby": "pnpm run build:clusterer && pnpm run build:infobox && pnpm run build:api",
    "start:gatsby": "cd packages/react-google-maps-api-gatsby-example && pnpm run start && cd ../..",
    "prestart:gatsby": "pnpm run build:clusterer && pnpm run build:infobox && pnpm run build:api",
    "clean": "pnpm run -r clean",
    "storybook": "start-storybook -p 6006",
    "bootstrap": "lerna bootstrap",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{ts|tsx}": [
      "pnpx eslint --fix",
      "tsc --noEmit --jsx react --esModuleInterop",
      "git add -A"
    ]
  },
